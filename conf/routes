GET     /                               controllers.Application.index
GET     /:id                            controllers.Application.visualization(id: String)

GET     /api/training/alone             controllers.Api.trainingAlone
POST    /api/:gameId/:token/play/:dir   controllers.Api.move(gameId: String, token: String, dir: String)

